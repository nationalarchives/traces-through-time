{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "TTT JSON Schema, v2.1",
  "description": "A person identified in a Traces Through Time dataset",
  "type": "object",
  "properties": {
    "comments": {
      "type": "string",
      "description": "notes or comments"
    },
    "aliveYear": { "type": "object",
      "description": "single 'floruit' date for a person inferred from the data",
      "minItems": 0,
      "properties": {
        "comments": {
          "type": "string",
          "description": "notes or comments"
        },
        "certain": {
          "type": "boolean",
          "description": "certainty of 'floruit' date"
        },
        "year": { "$ref": "#/definitions/year"
        }
      }
    },
    "aliveYearEnd": { "type": "object",
      "description": "end of 'floruit' date range for a person inferred from the data",
      "properties": {
        "comments": {
          "type": "string",
          "description": "notes or comments"
        },
        "certain": {
          "type": "boolean",
          "description": "certainty of end of 'floruit' date range"
        },
        "year": { "$ref": "#/definitions/year"
        }
      }
    },
    "aliveYearStart": { "type": "object",
      "description": "start of 'floruit' date range for a person inferred from the data",
      "properties": {
        "comments": {
          "type": "string",
          "description": "notes or comments"
        },
        "certain": {
          "type": "boolean",
          "description": "certainty of start of 'floruit' date range"
        },
        "year": { "$ref": "#/definitions/year"
        }
      }
    },
    "deadYear": { "type": "object",
      "description": "single deceased date for a person inferred from the data",
      "minItems": 0,
      "properties": {
        "comments": {
          "type": "string",
          "description": "notes or comments"
        },
        "certain": {
          "type": "boolean",
          "description": "certainty of deceased date"
        },
        "year": { "$ref": "#/definitions/year"
        }
      }
    },
    "hasAge": { "type": "object",
      "description": "person has an age",
      "properties": {
        "comments": {
          "type": "string",
          "description": "notes or comments"
        },
        "certain": {
          "type": "boolean",
          "description": "certainty of age identification"
        },
        "dateEnd": {
          "type": "number",
          "description": "end of dates covering age identification"
        },
        "dateStart": {
          "type": "number",
          "description": "start of date(s) covering age identification"
        },
        "type": {
          "type": "string",
          "description": "exactness of age identification",
          "enum": [
            "APPROX",
            "EXACT"
          ],
          "default": "APPROX"
        },
        "age": { "type": "object",
          "description": "age of person",
          "properties": {
            "comments": {
              "type": "string",
              "description": "notes or comments"
            },
            "end": {
              "type": "number",
              "description": "end of age range"
            },
            "name": {
              "type": "string",
              "description": "description of age"
            },
            "start": {
              "type": "number",
              "description": "start of age range"
            },
            "type": {
              "type": "string",
              "description": "exactness of age",
              "enum": [
                "APPROX",
                "EXACT"
              ],
              "default": "APPROX"
            }
          },
          "required": [
            "start"
          ]
        }
      }
    },
    "hasAliases": { "type": "array",
      "description": "person has alias(es)",
      "minItems": 0,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "certain": {
            "type": "boolean",
            "description": "certainty of alias identification"
          },
          "dateEnd": {
            "type": "number",
            "description": "end of use of alias"
          },
          "dateStart": {
            "type": "number",
            "description": "start of use of alias"
          },
          "type": {
            "type": "string",
            "description": "type of alias"
          },
          "alias": { "type": "object",
            "description": "alias of person",
            "properties": {
              "comments": {
                "type": "string",
                "description": "notes or comments"
              },
              "name": {
                "type": "string",
                "description": "name of alias"
              },
              "hasParticle": { "$ref": "#/definitions/hasParticle"
              }
            },
            "required": [
              "name"
            ]
          }
        }
      }
    },
    "hasAwards": { "type": "array",
      "description": "person has award(s)",
      "minItems": 0,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "certain": {
            "type": "boolean",
            "description": "certainty of award"
          },
          "dateEnd": {
            "type": "number",
            "description": "end of dates with award"
          },
          "dateStart": {
            "type": "number",
            "description": "start of date(s) with award"
          },
          "origin": {
            "type": "string",
            "description": "location or campaign where an individual award was won"
          },
          "award": { "$ref": "#/definitions/award"
          }
        }
      }
    },
    "hasChilds": { "type": "array",
      "description": "person has children",
      "minItems": 0,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "certain": {
            "type": "boolean",
            "description": "certainty of having child"
          },
          "dateEnd": {
            "type": "number",
            "description": "end of dates with child"
          },
          "dateStart": {
            "type": "number",
            "description": "start of date(s) with child"
          },
          "precedence": {
            "type": "integer",
            "description": "sequence of child"
          },
          "score": {
            "type": "number",
            "description": "confidence weighting for the relationship assertion"
          },
          "type": {
            "type": "string",
            "description": "relationship to child"
          },
          "person": { "$ref": "#"
          }
        }
      }
    },
    "hasEvents": { "type": "array",
      "description": "person experiences event(s)",
      "minItems": 0,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "certain": {
            "type": "boolean",
            "description": "certainty of experiencing the event"
          },
          "event": { "$ref": "#/definitions/event"
          }
        }
      }
    },
    "hasFamilyNames": { "type": "array",
      "description": "person has a family name",
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "certain": {
            "type": "boolean",
            "description": "certainty of person's possession of a family name"
          },
          "dateEnd": {
            "type": "number",
            "description": "end of person's possession of a family name"
          },
          "dateStart": {
            "type": "number",
            "description": "start of person's possession of a family name"
          },
          "order": {
            "type": "integer",
            "description": "formal order of person's multiple family names"
          },
          "precedence": {
            "type": "integer",
            "description": "chronological sequence of person's possession of different family names"
          },
          "preference": {
            "type": "integer",
            "description": "preference of person's usage of a family name"
          },
          "familyName": { "$ref": "#/definitions/familyName"
          }
        },
        "required": [
          "familyName",
          "order"
        ]
      }
    },
    "hasForeNames": { "type": "array",
      "description": "person has forename(s)",
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "certain": {
            "type": "boolean",
            "description": "certainty of person's possession of a forename"
          },
          "dateEnd": {
            "type": "number",
            "description": "end of person's possession of a forename"
          },
          "dateStart": {
            "type": "number",
            "description": "start of person's possession of a forename"
          },
          "order": {
            "type": "integer",
            "description": "formal order of person's multiple forenames"
          },
          "precedence": {
            "type": "integer",
            "description": "chronological sequence of person's possession of different forenames"
          },
          "preference": {
            "type": "integer",
            "description": "preference of person's usage of a forename"
          },
          "foreName": { "$ref": "#/definitions/foreName"
          }
        },
        "required": [
          "foreName",
          "order"
        ]
      }
    },
    "hasNationalities": { "type": "array",
      "description": "person has nationality/ies",
      "minItems": 0,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "certain": {
            "type": "boolean",
            "description": "certainty of nationality identification"
          },
          "dateEnd": {
            "type": "number",
            "description": "end of possession of a nationality"
          },
          "dateStart": {
            "type": "number",
            "description": "start of possession of a nationality"
          },
          "precedence": {
            "type": "integer",
            "description": "sequence of possession of a nationality"
          },
          "nationality": { "type": "object",
            "description": "nationality of person",
            "properties": {
              "comments": {
                "type": "string",
                "description": "notes or comments"
              },
              "name": {
                "type": "string",
                "description": "name of nationality"
              },
              "ref": {
                "type": "string",
                "description": "nationality identifier"
              }
            },
            "required": [
              "name"
            ]
          }
        }
      }
    },
    "hasOccupations": { "type": "array",
      "description": "person has occupation(s)",
      "minItems": 0,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "certain": {
            "type": "boolean",
            "description": "certainty of occupation"
          },
          "dateEnd": {
            "type": "number",
            "description": "end of dates with occupation"
          },
          "dateStart": {
            "type": "number",
            "description": "start of date(s) with occupation"
          },
          "precedence": {
            "type": "integer",
            "description": "sequence of occupation"
          },
          "occupation": { "$ref": "#/definitions/occupation"
          }
        }
      }
    },
    "hasParents": { "type": "array",
      "description": "person has parent(s)",
      "minItems": 0,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "certain": {
            "type": "boolean",
            "description": "certainty of having this parent"
          },
          "dateEnd": {
            "type": "number",
            "description": "end of dates having this parent"
          },
          "dateStart": {
            "type": "number",
            "description": "start of date(s) having this parent"
          },
          "precedence": {
            "type": "integer",
            "description": "sequence of parent"
          },
          "score": {
            "type": "number",
            "description": "confidence weighting for the relationship assertion"
          },
          "type": {
            "type": "string",
            "description": "relationship to parent"
          },
          "person": { "$ref": "#"
          }
        }
      }
    },
    "hasPlaces": { "$ref": "#/definitions/hasPlaces"
    },
    "hasReferences": { "type": "array",
      "description": "person has reference number(s)",
      "minItems": 0,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "certain": {
            "type": "boolean",
            "description": "certainty of possession of reference number"
          },
          "dateEnd": {
            "type": "number",
            "description": "end of possession of reference number"
          },
          "dateStart": {
            "type": "number",
            "description": "start of possession of reference number"
          },
          "reference": { "type": "object",
            "description": "reference number of person",
            "properties": {
              "comments": {
                "type": "string",
                "description": "notes or comments"
              },
              "genre": {
                "type": "string",
                "description": "genre of reference number of person"
              },
              "name": {
                "type": "string",
                "description": "name of reference number of person"
              },
              "type": {
                "type": "string",
                "description": "type of reference number of person"
              }
            },
            "required": [
              "name"
            ]
          }
        }
      }
    },
    "hasRelations": { "type": "array",
      "description": "person has non-immediate family relation(s)",
      "minItems": 0,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "certain": {
            "type": "boolean",
            "description": "certainty of having this non-immediate family relation"
          },
          "dateEnd": {
            "type": "number",
            "description": "end of dates having this non-immediate family relation"
          },
          "dateStart": {
            "type": "number",
            "description": "start of date(s) having this non-immediate family relation"
          },
          "name": {
            "type": "string",
            "description": "name of relationship to this non-immediate family relation"
          },
          "score": {
            "type": "number",
            "description": "confidence weighting for the relationship assertion"
          },
          "type": {
            "type": "string",
            "description": "type of relationship to this non-immediate family relation"
          },
          "person": { "$ref": "#"
          }
        }
      }
    },
    "hasRoles": { "type": "array",
      "description": "person has role(s) in or regarding something",
      "minItems": 0,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "certain": {
            "type": "boolean",
            "description": "certainty of involvement with the role"
          },
          "dateEnd": {
            "type": "number",
            "description": "end of involvement with the role"
          },
          "dateStart": {
            "type": "number",
            "description": "start of involvement with the role"
          },
          "name": {
            "type": "string",
            "description": "description or nature of the role"
          },
          "precedence": {
            "type": "integer",
            "description": "sequence of involvement with the role"
          },
          "event": { "$ref": "#/definitions/event"
          },
          "organisation": { "$ref": "#/definitions/organisation"
          },
          "person": { "$ref": "#"
          },
          "place": { "$ref": "#/definitions/place"
          }
        }
      }
    },
    "hasSiblings": { "type": "array",
      "description": "person has siblings",
      "minItems": 0,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "certain": {
            "type": "boolean",
            "description": "certainty of having sibling"
          },
          "dateEnd": {
            "type": "number",
            "description": "end of having sibling"
          },
          "dateStart": {
            "type": "number",
            "description": "start of having sibling"
          },
          "precedence": {
            "type": "integer",
            "description": "relative order of siblings where -1 etc. is younger and +1 etc. is older"
          },
          "score": {
            "type": "number",
            "description": "confidence weighting for the relationship assertion"
          },
          "type": {
            "type": "string",
            "description": "relationship to sibling"
          },
          "person": { "$ref": "#"
          }
        }
      }
    },
    "hasSpouses": { "type": "array",
      "description": "person has spouse(s)",
      "minItems": 0,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "certain": {
            "type": "boolean",
            "description": "certainty of having spouse"
          },
          "dateEnd": {
            "type": "number",
            "description": "end of having spouse"
          },
          "dateStart": {
            "type": "number",
            "description": "start of having spouse"
          },
          "precedence": {
            "type": "integer",
            "description": "sequence of spouse"
          },
          "score": {
            "type": "number",
            "description": "confidence weighting for the relationship assertion"
          },
          "type": {
            "type": "string",
            "description": "type of spouse relationship"
          },
          "person": { "$ref": "#"
          }
        }
      }
    },
    "hasTitles": { "type": "array",
      "description": "person has title(s)",
      "minItems": 0,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "certain": {
            "type": "boolean",
            "description": "certainty of having title"
          },
          "dateEnd": {
            "type": "number",
            "description": "end of having title"
          },
          "dateStart": {
            "type": "number",
            "description": "start of having title"
          },
          "precedence": {
            "type": "integer",
            "description": "sequence of title"
          },
          "title": { "$ref": "#/definitions/title"
          }
        }
      }
    },
    "inContainer": { "$ref": "#/definitions/inContainer"
    },
    "ref": { "type": "string",
      "description": "person identifier"
    },
    "sameAs": { "type": "array",
      "description": "another person that might be the 'same as' this person",
      "minItems": 0,
      "items": {
        "type": "object",
        "properties": {
          "comments": {
            "type": "string",
            "description": "notes or comments"
          },
          "genre": {
            "type": "string",
            "description": "how the 'same as' assertion was derived"
          },
          "id": {
            "type": "string",
            "description": "id of the relevant batch or result set stored in an auditing database, eg. MongoDB"
          },
          "score": {
            "type": "number",
            "description": "weighting for the 'same as' assertion"
          },
          "timestamp": {
            "type": "string",
            "description": "date/time the 'same as' assertion was made"
          },
          "type": {
            "type": "string",
            "description": "algorithmic methods employed in the 'same as' assertion"
          },
          "person": { "$ref": "#"
          }
        },
        "required": [
          "person",
          "score",
          "timestamp"
        ]
      }
    },
    "timestamp": { "type": "string",
      "description": "date/time person identified"
    },
    "type": { "type": "string",
      "description": "person gender",
      "enum": [
        "FEMALE",
        "MALE",
        "OTHER",
        "UNKNOWN"
      ],
      "default": "UNKNOWN"
    }
  },
  "required": [
    "inContainer",
    "ref",
    "timestamp",
    "type"
  ],



  "definitions": {
    "award": { "type": "object",
      "description": "person's award",
      "properties": {
        "comments": {
          "type": "string",
          "description": "notes or comments"
        },
        "name": {
          "type": "string",
          "description": "name of award"
        },
        "awardAliases": { "type": "array",
          "description": "award has alias(es)",
          "minItems": 0,
          "items": {
            "type": "object",
            "properties": {
              "comments": {
                "type": "string",
                "description": "notes or comments"
              },
              "type": {
                "type": "string",
                "description": "type of alias"
              },
              "award": { "$ref": "#/definitions/award"
              }
            }
          }
        }
      },
      "required": [
        "name"
      ]
    },
    "context": { "type": "object",
      "description": "immediate data context in which person is found",
      "properties": {
        "comments": {
          "type": "string",
          "description": "notes or comments"
        },
        "name": {
          "type": "string",
          "description": "immediate sentence, cell or 50 chars either side"
        },
        "ref": {
          "type": "string",
          "description": "original machine-readable or system identifier"
        },
        "hasDay": { "$ref": "#/definitions/hasDay"
        },
        "hasEndYear": { "$ref": "#/definitions/hasEndYear"
        },
        "hasMonth": { "$ref": "#/definitions/hasMonth"
        },
        "hasStartYear": { "$ref": "#/definitions/hasStartYear"
        },
        "hasTime": { "$ref": "#/definitions/hasTime"
        },
        "hasYear": { "$ref": "#/definitions/hasYear"
        },
        "inContainer": { "$ref": "#/definitions/inContainer"
        }
      },
      "required": [
        "name"
      ]
    },
    "day": { "type": "object",
      "description": "a single day",
      "properties": {
        "comments": {
          "type": "string",
          "description": "notes or comments"
        },
        "name": {
          "type": "string",
          "description": "description of day"
        },
        "value": {
          "type": "number",
          "description": "day",
          "minimum": 1,
          "maximum": 31
        },
        "hasMonth": { "$ref": "#/definitions/hasMonth"
        }
      },
      "required": [
        "value"
      ]
    },
    "document": { "type": "object",
      "description": "parent data structure (eg. webpage or spreadsheet) or a smaller, discrete unit within such a data structure (eg. paragraph or row)",
      "properties": {
        "comments": {
          "type": "string",
          "description": "notes or comments"
        },
        "filepath": {
          "type": "string",
          "description": "local file system path to any file that is the origin of the source data"
        },
        "name": {
          "type": "string",
          "description": "an original document reference no."
        },
        "quality": {
          "type": "string",
          "description": "indication of the quality and/or reliability of the source data"
        },
        "ref": {
          "type": "string",
          "description": "original machine-readable or system identifier"
        },
        "type": {
          "type": "string",
          "description": "nature or format of the container data structure"
        },
        "URL": {
          "type": "string",
          "description": "container webpage (or sub-section thereof if identified by a '#' fragment identifier)"
        },
        "hasDay": { "$ref": "#/definitions/hasDay"
        },
        "hasEndYear": { "$ref": "#/definitions/hasEndYear"
        },
        "hasMonth": { "$ref": "#/definitions/hasMonth"
        },
        "hasStartYear": { "$ref": "#/definitions/hasStartYear"
        },
        "hasTime": { "$ref": "#/definitions/hasTime"
        },
        "hasYear": { "$ref": "#/definitions/hasYear"
        },
        "heldBy": { "$ref": "#/definitions/heldBy"
        },
        "inContainer": { "$ref": "#/definitions/inContainer"
        }
      }
    },
    "event": { "type": "object",
      "description": "a personal event",
      "properties": {
        "comments": {
          "type": "string",
          "description": "notes or comments"
        },
        "genre": {
          "type": "string",
          "description": "genre of event"
        },
        "type": {
          "type": "string",
          "description": "name of event"
        },
        "hasDay": { "$ref": "#/definitions/hasDay"
        },
        "hasEndYear": { "$ref": "#/definitions/hasEndYear"
        },
        "hasMonth": { "$ref": "#/definitions/hasMonth"
        },
